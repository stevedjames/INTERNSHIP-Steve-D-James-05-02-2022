<% for(var i = 0; i < data.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= data[i].name %></td>
    <td><%= data[i].roll %></td>
    <td><%= data[i].mobile %></td>
    <td><%= data[i].standard %></td>
    <td><%= data[i].division %></td>
    <td>
        <a href="/update-student/<%= data[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" href="/student-delete/<%= data[i]._id%>">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>